{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch craft",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/craft",
            "cwd": "${workspaceFolder}/cmd/ast/",
            "args": ["-by", "github.com/aira3ppp/craft/example/macros/hello", "-to", "hello.gen.go"],
            "env": {
                "GOFILE": "ast.go",
                "GOLINE": "69",
                "GOPACKAGE": "dog_pkg",
                "DOLLAR": "d$g",
            }
        },
        {
            "name": "Launch craft with xxxxxxxxxxxxxxxxx",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/craft",
            "cwd": "${workspaceFolder}/cmd/ast/",
            "args": ["xxxxxxxxxxxxxxxxx"],
            "env": {
                "GOFILE": "ast.go",
                "GOLINE": "69",
                "GOPACKAGE": "dog_pkg",
                "DOLLAR": "d$g",
            }
        }
    ]
}